{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"eofline": false,
		"max-line-length": [
			true,
			120
		],
		"member-access": false,
		"array-type": [
			false
		],
		"ordered-imports": [
			false
		],
		"object-literal-sort-keys": false,
		"no-var-requires": false,
		"interface-name": [
			false
		]
	},
	"rulesDirectory": []
}

